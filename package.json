{
  "name": "aha-develop.assign-devin-ai",
  "description": "Devin.ai",
  "version": "1.0.2",
  "author": "Aha! (support@aha.io)",
  "repository": {
    "type": "git",
    "url": "https://github.com/aha-develop/assign-devin"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^16.14.68",
    "aha-cli": "^1.17.0",
    "prettier": "^3.8.0"
  },
  "ahaExtension": {
    "contributes": {
      "views": {
        "sendToDevinButton": {
          "title": "Devin",
          "entryPoint": "src/views/SendToDevinButton.tsx",
          "host": "attribute",
          "recordTypes": [
            "Feature",
            "Requirement"
          ]
        }
      },
      "events": {
        "createDevinSession": {
          "title": "Create Devin Session",
          "entryPoint": "src/events/createDevinSession.ts",
          "handles": [
            "aha-develop.assign-devin-ai.createDevinSession"
          ]
        }
      },
      "settings": {
        "repository": {
          "title": "GitHub Repository",
          "description": "Format: owner/repo (e.g., myorg/myrepo). Ensure you have Devin integrated with this repository https://app.devin.ai/settings/repositories",
          "type": "string",
          "required": true,
          "scope": [
            "account",
            "user"
          ]
        },
        "baseBranch": {
          "title": "Base Branch",
          "description": "Repositories default branch",
          "type": "string",
          "default": "main",
          "scope": [
            "account",
            "user"
          ]
        },
        "apiKey": {
          "title": "Service API Key",
          "description": "Organization-scoped service key for automation",
          "type": "secret",
          "scope": [
            "account"
          ],
          "required": true
        },
        "personalApiKey": {
          "title": "Personal API Key",
          "description": "Personal API keys are tied to individual user accounts and scoped to an org.",
          "type": "secret",
          "scope": [
            "user"
          ]
        },
        "sessionTags": {
          "title": "Default Session Tags",
          "description": "Comma-separated list of tags to apply to new Devin sessions.",
          "type": "string",
          "scope": [
            "account",
            "user"
          ]
        },
        "customInstructions": {
          "title": "Custom Instructions",
          "description": "Optional extra guidance appended to the session prompt.",
          "type": "string",
          "scope": [
            "account",
            "user"
          ]
        },
        "playbookId": {
          "title": "Playbook ID",
          "description": "Optional Devin playbook identifier to attach to new sessions.",
          "type": "string",
          "scope": [
            "account",
            "user"
          ]
        }
      }
    }
  },
  "dependencies": {
    "zod": "^4.3.5"
  }
}
